#include <iostream>
using namespace std;

int main(){
    int y = 2;
    int d = 3;
    bool f1 = (y==d);
    cout<<f1<<endl;

    bool f2 = (y>d);
    cout<<f2<<endl;

    bool f3 = (y>=d);
    cout<<f3<<endl;

    bool f4 = (y<=d);
    cout<<f4<<endl;

    bool f5 = (y<d);
    cout<<f5<<endl;

    bool f6 = (y!=d);
    cout<<f6<<endl;

    //logical operation
    int a = 21;
    cout<<!a<<endl;

}
